{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": "off",
		"no-tabs": "off",
		"newline-per-chained-call": "off",
		"comma-dangle": "off",
		"no-unused-vars": "warn",
		"no-console": "warn"
	}
}
